{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AppConfig": {
    "Service": "C#-Logger",
    "Host": "http://localhost:8085",
    "Modules": {
      "wapper": [
        {
          "code": "RustAuth",
          "active": false,
          "arguments": {
            "host": "http://localhost:3000",
            "pass_key": "4ac6f724-d97b-47c5-a56a-be151b42da6a",
            "manager": "AES_GCM",
            "size": 256,
            "nonce": ""
          }
        }
      ],
      "closeable": [
        {
          "code": "Kafka",
          "active": true,
          "arguments": {
            "host": "localhost:9093",
            "group_id": "CLOGGER",
            "topic": "CLogger"
          }
        }
      ]
    },
    "Persistence": {
      "code": "RepositoryMySQL",
      "connection": "Server=localhost;Port=3306;User ID=root;Password=example;",
      "database": "logger",
      "collection": "logs"
    }
  }
}
