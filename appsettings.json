{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AppConfig": {
    "WebHandlers": [
      "TestHandler"
    ],
    "ServiceEvents": [
      {
        "code": "TestEvent",
        "delay": 2000
      }
    ],
    "Persistence": {
      "code": "RepositoryMySQL",
      "connection": "Server=localhost;Port=3306;User ID=root;Password=example;",
      "database": "logger",
      "collection": "logs"
    }
  }
}
