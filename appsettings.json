{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AppConfig": {
    "Service": "C#-Logger",
    "Host": "http://localhost:8085",
    "ServiceEvents": [
      {
        "code": "TestEvent",
        "delay": 2000
      }
    ],
    "Modules": [
      {
        "code": "RustAuth",
        "active": true,
        "arguments": {
          "host": "http://localhost:3000",
          "pass_key": "4a8d4039-f785-4bb3-852c-8b9c43d55ee7",
          "manager": "AES_GCM",
          "size": 256,
          "nonce": ""
        }
      }
    ],
    "Persistence": {
      "code": "RepositoryMySQL",
      "connection": "Server=localhost;Port=3306;User ID=root;Password=example;",
      "database": "logger",
      "collection": "logs"
    }
  }
}
